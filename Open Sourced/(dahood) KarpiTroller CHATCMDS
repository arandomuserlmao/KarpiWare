PPlocation.GripPos = Vector3.new(-10,10,10)
plr.Character.Humanoid:UnequipTools()
plr.Backpack[PPname].Parent = plr.Character
PPlocation:Activate()
plr.Character.Humanoid:UnequipTools()
end
end)
                        end
                    end
                )
            end
