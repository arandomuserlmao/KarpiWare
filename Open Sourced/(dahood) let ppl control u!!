                         for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
                plr.Chatted:Connect(
                    function(Chat)
                     local plr1 = game.Players.LocalPlayer
                        if Chat == ".bring" then
local plr1 = game.Players.LocalPlayer.Character
local plr2 = plr.Character
plr1.HumanoidRootPart.CFrame = plr2.HumanoidRootPart.CFrame * CFrame.new(0,2,0)
                        end
                    end
                )
            end

                                     for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
                plr.Chatted:Connect(
                    function(Chat)
                     local plr1 = game.Players.LocalPlayer
                        if Chat == ".drop" then
  local humanoid = game.Players.LocalPlayer.Character.Humanoid
                            local tool = game.Players.LocalPlayer.Backpack["[Money Gun]"]

                            humanoid:EquipTool(tool)
                            tool:Activate()
                            humanoid:UnequipTools(tool)
                        end
                    end
                )
            end

                                     for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
                plr.Chatted:Connect(
                    function(Chat)
                     local plr1 = game.Players.LocalPlayer
                        if Chat == ".freeze" then
ply = game.Players.LocalPlayer.Character.UpperTorso
ply2 = game.Players.LocalPlayer.Character.HumanoidRootPart
ply.Anchored = true
ply2.Anchored = true
                        end
                    end
                )
            end

                                                 for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
                plr.Chatted:Connect(
                    function(Chat)
                     local plr1 = game.Players.LocalPlayer
                        if Chat == ".unfreeze" then
ply = game.Players.LocalPlayer.Character.UpperTorso
ply2 = game.Players.LocalPlayer.Character.HumanoidRootPart
ply.Anchored = false
ply2.Anchored = false
                        end
                    end
                )
            end

                                     for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
                plr.Chatted:Connect(
                    function(Chat)
                     local plr1 = game.Players.LocalPlayer
                        if Chat == ".kill" then
local plr1 = game.Players.LocalPlayer.Character
local plr2 = plr.Character
plr1.Humanoid.Health = 0
                        end
                    end
                )
            end

                                                 for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
                plr.Chatted:Connect(
                    function(Chat)
                     local plr1 = game.Players.LocalPlayer
                        if Chat == ".flop" then
local character = game.Players.LocalPlayer.Character
local Humanoid = character.Humanoid
Humanoid.PlatformStand = true
wait(3)
Humanoid.PlatformStand = false
                        end
                    end
                )
            end
